          <QueryParameter Name="@FinalDate">
            <Value>=Parameters!FinalDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SalesSummaryId, CustomerId, CustomerName, SummaryDate, Total_Items, Total_Sales, Source
FROM dbo.vw_SalesSummaryReport
WHERE SummaryDate &gt;= @InitialDate AND SummaryDate &lt;= @FinalDate
ORDER BY SummaryDate, CustomerId</CommandText>
